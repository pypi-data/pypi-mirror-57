"%(err_reason)s:\n"
"cabeçalhos de solicitação: %(reqheaders)s\n"
"\n"
"corpo da solicitação: %(reqbody)s\n"
"\n"
"cabeçalhos de resposta: %(respheaders)s\n"
"\n"
"corpo de resposta: %(respbody)s"

#: pypowervm/adapter.py:1176
#, python-format
msgid "Atom error for %(method)s %(path)s: %(reason)s"
msgstr "Erro do Atom para %(method)s %(path)s: %(reason)s"

#: pypowervm/adapter.py:1217
msgid "Session must not be None"
msgstr "A sessão não deve ser Nenhuma"

#: pypowervm/adapter.py:1219
msgid "An event listener is already active on the session."
msgstr "Um listener de eventos já está ativo na sessão."

#: pypowervm/adapter.py:1238
#, python-format
msgid "Failed to initialize event feed listener: %s"
msgstr "Falha ao inicializar o listener de feed de eventos: %s"

#: pypowervm/adapter.py:1242
