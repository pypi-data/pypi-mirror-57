#!/usr/bin/env python
import os
import sys

if len(sys.argv) < 2:
    print("Usage: {} <cluspro_id>".format(os.path.basename(sys.argv[0])))
    sys.exit(1)

cluspro_id = sys.argv[1]

import pymol
pymol.pymol_argv = ['pymol', '-q'] + sys.argv[2:]
pymol.finish_launching()
cmd = pymol.cmd

from sblu.pymol import importing
importing.cl_load_job(cluspro_id)
