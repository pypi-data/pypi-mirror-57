#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'snipco==0.2','console_scripts','snipco'
__requires__ = 'snipco==0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('snipco==0.2', 'console_scripts', 'snipco')()
    )
